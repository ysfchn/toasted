{
    "arguments": "foo",
    "toast_id": "mytoast",
    "group_id": "mygroup",
    "elements": [
        {
            "@type": "text",
            "content": "Send Picture"
        },
        {
            "@type": "input",
            "id": "inputMail",
            "placeholder": "example@example.com",
            "title": "E-mail address"
        },
        {
            "@type": "input",
            "id": "inputMessage",
            "default": "Hello world!",
            "title": "Message"
        },
        {
            "@type": "select",
            "id": "select1",
            "options": {
                "yes": "Yes",
                "false": "No"
            },
            "default": "yes",
            "title": "Encrypt message"
        },
        {
            "@type": "button",
            "input_id": "inputMail",
            "content": "Save to contacts",
            "arguments": "save"
        },
        {
            "@type": "image",
            "source": "https://picsum.photos/id/675/650/350"
        },
        {
            "@type": "button",
            "content": "Change image",
            "arguments": "change"
        },
        {
            "@type": "button",
            "content": "Send",
            "arguments": "send"
        }
    ]
}